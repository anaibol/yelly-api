{
  "name": "yelly-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "NODE_ENV=development DEBUG=@neo4j/graphql:execute nest start --watch",
    "dev:docker:up": "docker compose -f docker-compose.dev.yml --env-file .env.docker.dev up",
    "dev:docker:up:reset": "rm -R -f db && yarn dev:docker:up",
    "dev:docker:down": "docker compose -f docker-compose.dev.yml --env-file .env.docker.dev down",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "import:algolia:cities": "npx ts-node scripts/algolia/importCities.ts",
    "import:algolia:schools": "npx ts-node scripts/algolia/importSchools.ts",
    "import:algolia:tags": "npx ts-node scripts/algolia/importTags.ts",
    "import:algolia:users": "npx ts-node scripts/algolia/importUsers.ts",
    "neo:serve": "npx ts-node src/neo/serve.ts",
    "neo:generate": "npx ts-node src/neo/generate.ts",
    "prisma:studio": "npx prisma studio --experimental",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch"
  },
  "dependencies": {
    "@amplitude/node": "^1.10.2",
    "@anchan828/nest-bullmq": "^2.0.9",
    "@aws-sdk/client-s3": "3.168.0",
    "@aws-sdk/s3-request-presigner": "3.168.0",
    "@aws-sdk/signature-v4-crt": "3.168.0",
    "@fastify/accepts": "^4.0.1",
    "@mailchimp/mailchimp_transactional": "^1.0.47",
    "@nestjs/apollo": "^10.1.0",
    "@nestjs/cli": "9.1.2",
    "@nestjs/common": "9.0.11",
    "@nestjs/core": "9.0.11",
    "@nestjs/graphql": "^10.1.1",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "^9.0.11",
    "@prisma/client": "^4.3.1",
    "@tracworx/nestjs-dataloader": "^1.1.1",
    "@types/date-fns": "^2.6.0",
    "algoliasearch": "^4.14.2",
    "apollo-server": "^3.6.5",
    "apollo-server-core": "3.5.0",
    "apollo-server-express": "^3.10.2",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bullmq": "^1.90.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dataloader": "^2.1.0",
    "date-fns": "2.29.2",
    "dotenv": "16.0.2",
    "expo-server-sdk": "^3.6.0",
    "express": "^4.17.1",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "nanoid": "3.3.4",
    "nestjs-http-promise": "^2.0.0",
    "nestjs-i18n": "^9.1.10",
    "p-all": "4.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "qs": "^6.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "7.5.6",
    "twilio": "3.82.0",
    "uuid": "9.0.0",
    "yoti": "^3.22.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@amplitude/analytics-node": "^0.5.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.0.11",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.0",
    "@types/google.maps": "^3.50.0",
    "@types/jest": "^29.0.1",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.16",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-functional": "^4.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "source-map-support": "^0.5.21",
    "sql-formatter": "^10.6.0",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tsutils": "^3.21.0",
    "typescript": "^4.8.3",
    "webpack": "5.74.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
