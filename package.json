{
  "name": "yelly-nest-poc",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "import:algolia:cities": "npx ts-node scripts/algolia/importCitys.ts",
    "import:algolia:schools": "npx ts-node scripts/algolia/importSchools.ts",
    "import:algolia:tags": "npx ts-node scripts/algolia/importTags.ts",
    "import:algolia:users": "npx ts-node scripts/algolia/importUsers.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.44.0",
    "@aws-sdk/s3-request-presigner": "3.44.0",
    "@aws-sdk/signature-v4-crt": "3.41.0",
    "@mailchimp/mailchimp_transactional": "^1.0.46",
    "@nestjs/cli": "8.1.5",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "9.1.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "^3.3.0",
    "algoliasearch": "^4.11.0",
    "apollo-server-core": "3.5.0",
    "apollo-server-express": "^3.4.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "3.6.0",
    "class-transformer": "^0.4.0",
    "class-validator": "0.13.2",
    "expo-server-sdk": "^3.6.0",
    "express": "4.17.1",
    "graphql": "^15.6.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "qs": "^6.10.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "7.4.0",
    "uuid": "8.3.2"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "3.4.2",
    "@types/express": "^4.17.13",
    "@types/google.maps": "^3.46.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/passport-local": "1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.6",
    "prettier": "^2.3.2",
    "prisma": "^3.3.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-morph": "13.0.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "5.65.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
