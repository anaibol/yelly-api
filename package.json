{
  "name": "yelly-nest-poc",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "NODE_ENV=development DEBUG=@neo4j/graphql:execute nest start --watch",
    "dev:docker:up": "docker compose -f docker-compose.dev.yml --env-file .env.docker.dev up",
    "dev:docker:up:reset": "rm -R -f db && yarn dev:docker:up",
    "dev:docker:down": "docker compose -f docker-compose.dev.yml --env-file .env.docker.dev down",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "import:algolia:cities": "npx ts-node scripts/algolia/importCities.ts",
    "import:algolia:schools": "npx ts-node scripts/algolia/importSchools.ts",
    "import:algolia:tags": "npx ts-node scripts/algolia/importTags.ts",
    "import:algolia:users": "npx ts-node scripts/algolia/importUsers.ts",
    "neo:serve": "npx ts-node src/neo/serve.ts",
    "neo:generate": "npx ts-node src/neo/generate.ts",
    "prisma:studio": "npx prisma studio --experimental",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch"
  },
  "dependencies": {
    "@amplitude/node": "^1.9.2",
    "@anchan828/nest-bullmq": "^2.0.2",
    "@aws-sdk/client-s3": "3.44.0",
    "@aws-sdk/s3-request-presigner": "3.44.0",
    "@aws-sdk/signature-v4-crt": "3.41.0",
    "@mailchimp/mailchimp_transactional": "^1.0.46",
    "@nestjs/apollo": "10.0.5",
    "@nestjs/cli": "8.2.1",
    "@nestjs/common": "8.3.1",
    "@nestjs/core": "8.3.1",
    "@nestjs/graphql": "^10.0.18",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-fastify": "^8.4.1",
    "@prisma/client": "^4.0.0",
    "@tracworx/nestjs-dataloader": "^1.1.0",
    "@types/date-fns": "^2.6.0",
    "algoliasearch": "^4.11.0",
    "apollo-server": "^3.6.5",
    "apollo-server-core": "3.5.0",
    "apollo-server-fastify": "^3.6.5",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "bullmq": "^1.86.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dataloader": "^2.0.0",
    "date-fns": "2.25.0",
    "dotenv": "14.2.0",
    "expo-server-sdk": "^3.6.0",
    "fastify": "^3.27.4",
    "graphql": "^15.6.1",
    "lodash": "^4.17.21",
    "nanoid": "3.3.4",
    "nestjs-http-promise": "^1.2.1",
    "nestjs-i18n": "^8.2.2",
    "p-all": "3.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "qs": "^6.10.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "7.4.0",
    "twilio": "3.73.0",
    "uuid": "8.3.2",
    "yoti": "^3.21.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@amplitude/analytics-node": "^0.3.4",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^1.7.3",
    "@types/google.maps": "^3.46.1",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-functional": "^4.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.6",
    "prettier": "^2.3.2",
    "prisma": "^4.0.0",
    "source-map-support": "^0.5.20",
    "sql-formatter": "^4.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^3.10.1",
    "tsutils": "^3.21.0",
    "typescript": "^4.7.3",
    "webpack": "5.65.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
